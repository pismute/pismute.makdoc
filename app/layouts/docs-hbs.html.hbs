{{{contents}}}

<script
    src="/bower_components/handlebars/handlebars.min.js"></script>
<script
    src="/bower_components/dashbars/dist/dashbars.min.js"></script>
<script defer>
    Dashbars.help(Handlebars);

    $('.lang-hbs').each(function(index){
        var data = window.data || $(this).attr('data') || {};
        var src = $(this).text();
        var template = Handlebars.compile(src);
        $(this).text(template(data));
    });
</script>
